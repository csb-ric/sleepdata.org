- @title = "New Legal Document Dataset"

- render "themes/theme"
- content_for :sidebar, render("admin/sidebar")
- content_for :header, @title

- content_for :breadcrumbs do
  %ol
    %li= link_to @organization.name, @organization
    %li= link_to "Legal Document Datasets", organization_legal_document_datasets_path(@organization)
    %li.breadcrumb-muted new

.dashboard-container= render "legal_document_datasets/form", legal_document_dataset: @legal_document_dataset
