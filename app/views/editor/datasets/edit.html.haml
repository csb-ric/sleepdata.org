- @title = "Edit Dataset: #{@dataset.name_was}"

- content_for :header do
  .header-container
    .container
      %h1.page-heading
        = @dataset.name_was
  .breadcrumb-container.mb-0
    .container
      %ol
        %li= link_to "Datasets", datasets_path
        %li= link_to @dataset.name_was, @dataset
        %li Edit

= render "datasets/form", dataset: @dataset
