- @title = "Edit Dataset: #{@dataset.name_was}"

- render "themes/theme"
= render "datasets/header", container: "container-fluid"
- content_for :sidebar, render("editor/datasets/sidebar")

- content_for :breadcrumbs do
  %ol
    %li= link_to "Datasets", datasets_path
    %li= link_to @dataset.name_was, @dataset
    %li Edit

.dashboard-container= render "datasets/form", dataset: @dataset
