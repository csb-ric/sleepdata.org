- @title = "Edit Dataset: #{@dataset.name_was}"

- render "themes/theme"
- content_for :header, @dataset.name_was
- content_for :header_append, render("datasets/header", container: "container-fluid")
- content_for :sidebar, render("editor/datasets/sidebar")

- content_for :breadcrumbs do
  %ol
    %li= link_to @dataset.slug_was.tr("-", " "), @dataset
    %li.breadcrumb-muted edit

.dashboard-container= render "datasets/form", dataset: @dataset
