<div class="page-header">
  <h1>Admin Dashboard</h1>
</div>

<div class="row">

  <div class="col-sm-6 col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><span class="glyphicon glyphicon-inbox"></span> Agreements Review</h3>
      </div>
      <div class="panel-body">
        <%= link_to 'Go To Agreements', agreements_path %>
      </div>
    </div>
  </div>

  <div class="col-sm-6 col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><span class="glyphicon glyphicon-user"></span> User Adminstration</h3>
      </div>
      <div class="panel-body">
        <%= link_to 'Go To Users', users_path %>
      </div>
    </div>
  </div>

  <div class="col-sm-6 col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><span class="glyphicon glyphicon-stats"></span> Website Statistics</h3>
      </div>
      <div class="panel-body">
        <%= link_to 'Go To Stats', stats_path %>
      </div>
    </div>
  </div>

  <div class="col-sm-6 col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><span class="glyphicon glyphicon-globe"></span> Where is Everyone From?</h3>
      </div>
      <div class="panel-body">
        <%= link_to 'Go To Location', location_path %>
      </div>
    </div>
  </div>

  <div class="col-sm-6 col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><span class="glyphicon glyphicon-repeat"></span> Sync Documentation</h3>
      </div>
      <div class="panel-body">
        <%= link_to 'Go To Sync', sync_path %>
      </div>
    </div>
  </div>

</div>
