- @title = "New Dataset"

- content_for :header do
  .header-container
    .container
      %h1.page-heading
        = @title
- content_for :breadcrumbs do
  %ol
    - if @organization
      %li= link_to @organization.name, @organization
    %li= link_to "Datasets", :datasets # [@organization, :datasets]
    %li{ style: "font-weight: 300; color: #9e9e9e;" } New

.dashboard-container= render "datasets/form", dataset: @dataset
