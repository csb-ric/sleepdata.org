- @title = "Edit User: #{@user.name_was}"

- content_for :header do
  .header-container
    .container
      %h1.page-heading
        = @user.name_was
  .breadcrumb-container.mb-0
    .container
      %ol
        %li= link_to "Users", users_path
        %li= link_to @user.name_was, @user
        %li Edit

.dashboard-container2
  = render "form", user: @user
