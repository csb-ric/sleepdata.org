- @title = "Legal Templates"

- content_for :header do
  .header-container
    .container
      %h1.page-heading
        .pull-right
          = render "layouts/edit_delete_dropdown", edit_url: edit_organization_path(@organization), delete_url: @organization, delete_confirmation: "Delete #{@organization.name}?"
        = @title

%table.table.table-striped
  %tr
    %td= link_to "Legal Template One", legal_template_one_organization_path(@organization)
  %tr
    %td= link_to "Legal Template Two", legal_template_two_organization_path(@organization)

