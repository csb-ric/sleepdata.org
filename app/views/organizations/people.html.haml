- @title = @organization.name

- render "themes/theme"
- content_for :container_class, "container-fluid"
- content_for :sidebar, render("admin/sidebar")
- content_for :header, @title

- content_for :breadcrumbs do
  %ol
    %li= link_to "Organizations", organizations_path
    %li= link_to @organization.name, @organization
    %li{ style: "font-weight: 300; color: #9e9e9e;" } People
    %li= link_to "Invite", invite_member_organization_path(@organization)

.dashboard-container.dashboard-table
  %table.table.table-striped.table-borderless.table-sticky.table-sm
    - @users.each do |user|
      %tr
        %td= user.username
        %td Owner

