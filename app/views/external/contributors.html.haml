- @title = 'Past Contributors'

- content_for :body_class do
  brand-background

- content_for :header do
  .header-container
    .container
      %h1.page-heading
        = @title

%p.lead.center
  A special thanks to our past contributors.

.center
  - User.current.where(contributor: true).order(:first_name, :last_name).each do |user|
    %p= user.name
