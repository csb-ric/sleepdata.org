- @title = @dataset.name
= render "datasets/header"


- content_for :header do
  - if @dataset.info_count > 0
    .header-container{ style: "padding: 0;" }
      .container
        = render "datasets/quick_info", dataset: @dataset
  .d-flex.justify-content-center.data-request-tab-sticky
    .background-fixed-image.data-request-tab-background
      = render "datasets/request", dataset: @dataset

= render "documentation/page", object: @dataset
