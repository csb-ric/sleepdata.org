- if current_user && @dataset.editable_by?(current_user)
  %ul.left-menu{ style: "margin-top: 20px" }
    %li.left-menu-header
      Admin
    %li= link_to "Agreements", agreements_dataset_path(@dataset), class: current_page?(agreements_dataset_path(@dataset)) ? "active" : nil
    %li= link_to "Audits", audits_dataset_path(@dataset), class: current_page?(audits_dataset_path(@dataset)) ? "active" : nil
    %li= link_to "Page Views", page_views_dataset_path(@dataset), class: current_page?(page_views_dataset_path(@dataset)) ? "active" : nil
    %li= link_to "Collaborators", collaborators_dataset_path(@dataset), class: current_page?(collaborators_dataset_path(@dataset)) ? "active" : nil
    %li= link_to "Sync", sync_dataset_path(@dataset), class: current_page?(sync_dataset_path(@dataset)) ? "active" : nil
