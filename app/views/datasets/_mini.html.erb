<% if dataset.logo.size > 0 %>
  <%= link_to image_tag( logo_dataset_path(dataset), style: 'max-width:100%;max-height:20px' ), dataset, rel: 'tooltip', title: dataset.name, data: { container: 'body', placement: 'top' } %>
<% else %>
  <%= link_to image_tag( 'project-default-logo.png', style: 'max-width:100%;max-height:20px' ), dataset, rel: 'tooltip', title: dataset.name, data: { container: 'body', placement: 'top' } %>
<% end %>
