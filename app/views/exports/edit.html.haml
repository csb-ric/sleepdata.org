- @title = "Edit Export: #{@export.name_was}"

- render "themes/theme"
- content_for :container_class, "container-fluid"
- content_for :sidebar, render("admin/sidebar")
- content_for :header, @export.name_was

- content_for :breadcrumbs do
  %ol
    %li= link_to "Exports", exports_path
    %li= link_to @export.name_was, @export
    %li Edit

.dashboard-container= render "exports/form", export: @export
